.title kicad schematic

.include NMOS-180nm.lib
.include userDiode.lib
.include PMOS-180nm.lib
* u4 input1 input2 gnd bias_current output vcc port
v1 vcc gnd 12v
* u6 vcc plot_v1
d2 vcc net-_d2-pad2_ D1N750
m6 net-_m5-pad3_ net-_d3-pad2_ vcc vcc CMOSP W=100u L=100u M=1
m7 gnd net-_d4-pad1_ net-_m3-pad3_ gnd CMOSN W=100u L=100u M=1
* u5 gnd plot_v1
d4 net-_d4-pad1_ gnd D1N750
m9 output net-_m3-pad3_ net-_d4-pad1_ gnd CMOSN W=100u L=100u M=1
* u7 output plot_v1
m8 net-_d3-pad2_ net-_m5-pad3_ output vcc CMOSP W=100u L=100u M=1
d3 vcc net-_d3-pad2_ D1N750
d1 bias_current gnd D1N750
r1 input2 net-_m5-pad2_ 1g
* u3 input1 plot_v1
m3 net-_d2-pad2_ net-_m1-pad1_ net-_m3-pad3_ vcc CMOSP W=100u L=100u M=1
m2 net-_m1-pad1_ net-_d2-pad2_ vcc vcc CMOSP W=100u L=100u M=1
m1 net-_m1-pad1_ input1 net-_m1-pad3_ gnd CMOSN W=100u L=100u M=1
m5 net-_m1-pad3_ net-_m5-pad2_ net-_m5-pad3_ gnd CMOSN W=100u L=100u M=1
m4 net-_m1-pad3_ bias_current gnd gnd CMOSN W=100u L=100u M=1
* u2 input2 plot_v1
* u1 bias_current plot_v1
.tran 0.1e-03 100e-03 0e-00

* Control Statements 
.control
run
print allv > plot_data_v.txt
print alli > plot_data_i.txt
plot v(vcc)
plot v(gnd)
plot v(output)
plot v(input1)
plot v(input2)
plot v(bias_current)
.endc
.end
